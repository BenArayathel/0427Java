package eg1;

public class Demo {

	public static void main(String[] args) {

		int x=10;
		int y=0;
		int res=0;
		try {
		res = x/y;
		System.out.println("result is "+res);
		} catch(ArithmeticException e) {
			System.out.println("You cannot divide by zero");
		}
		
		System.out.println("Thank you for using our app");
	}

}
