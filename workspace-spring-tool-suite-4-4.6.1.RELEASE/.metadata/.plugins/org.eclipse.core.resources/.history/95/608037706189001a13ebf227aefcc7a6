package com.example.inheritance;

public class Upcasting {
	
	public static void main(String[] args) {
/*
 * 
 * implicit - compiler -upcasting -child class obj is assigned to parent
 * explicit - dev -explicitly we have to say that the parent object needs to be converted to which child
 */
	Object o = new Child(); //reference variable not an obj
	System.out.println(o.getClass());
	System.out.println(o.hashCode());
	
	Parent p =(Parent) o; //Downcasting
	p.grandParentMethod();
	p.parentMethod();
	System.out.println(p.hashCode());
}
}